# Environment Variables Configuration
# Copy this file to .env and update the values according to your setup

# API Configuration
# The base URL for your backend API server
# Development: http://localhost:5000/api
# Production: https://your-api-domain.com/api
REACT_APP_API_URL=http://localhost:5000/api

# Add other environment variables here as needed
# Examples:

# Database Configuration (if needed for API calls)
# REACT_APP_DB_HOST=localhost
# REACT_APP_DB_PORT=5432

# Third-party Services
# REACT_APP_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here
# REACT_APP_STRIPE_PUBLISHABLE_KEY=pk_test_your_stripe_key_here

# Feature Flags
# REACT_APP_ENABLE_CHAT=true
# REACT_APP_ENABLE_NOTIFICATIONS=true

# Authentication
# REACT_APP_AUTH_DOMAIN=your-auth-domain.com
# REACT_APP_CLIENT_ID=your_client_id_here

# Analytics
# REACT_APP_GOOGLE_ANALYTICS_ID=GA-XXXXXXXXX
# REACT_APP_MIXPANEL_TOKEN=your_mixpanel_token_here